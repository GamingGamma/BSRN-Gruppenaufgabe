import time

def blink_ergebnis():
    while True: #endlosschleife
        print("Gewonnen!", end="\r")  # Zeilenumbruch verhindern
        time.sleep(0.5)  # Pausenzeit kann auch angepasst werden
        print("         ", end="\r")  # Leerzeichen Ã¼berschreiben das wort =Blinkeffekt
        time.sleep(0.5)  # Pausenzeit 

blink_ergebnis()